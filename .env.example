# Discord configuration (for bot)
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_BOT_CLIENT_ID=your_discord_bot_client_id_here

# Discord OAuth (for setup flow)
DISCORD_OAUTH_CLIENT_ID=your_discord_oauth_app_client_id_here
DISCORD_OAUTH_CLIENT_SECRET=your_discord_oauth_app_client_secret_here

# GitHub OAuth (for setup flow)
GITHUB_OAUTH_CLIENT_ID=your_github_oauth_app_client_id_here
GITHUB_OAUTH_CLIENT_SECRET=your_github_oauth_app_client_secret_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/githubbot
ENCRYPTION_KEY=your_64_character_hex_encryption_key_here

# Server
NODE_ENV=production
PORT=3000
